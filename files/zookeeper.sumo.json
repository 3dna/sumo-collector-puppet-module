{
    "api.version": "v1",
    "sources": [
        {
            "name": "Syslog Logs",
            "sourceType": "LocalFile",
            "automaticDateParsing": true,
            "multilineProcessingEnabled": false,
            "useAutolineMatching": true,
            "category": "OS/Linux/System",
            "pathExpression": "/var/log/syslog",
            "cutoffRelativeTime": "-30d"
        },
        {
